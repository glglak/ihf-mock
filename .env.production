# Production environment variables

# API Configuration
REACT_APP_API_URL=https://api.example.com/v1

# Use mock data (should be false in production)
REACT_APP_USE_MOCK_DATA=false

# Base URL for assets
REACT_APP_ASSET_URL=https://assets.example.com

# Debug mode
REACT_APP_DEBUG=false

# Google Analytics ID (production)
REACT_APP_GA_ID=UA-XXXXXXXXX-2

# Base path for the application
PUBLIC_URL=/

# Other configuration
REACT_APP_VERSION=$npm_package_version